{%comment%}
<header class="site-header" role="banner">
  {% if settings.display_header_top_text %}
  {% if settings.header_top_text != blank %}
  <div class="header-top">
    <div class="container">
      <div class="header-content" style="color:{{settings.text_header_top_color}}">
        <p class="mb-0"><i class="fas fa-truck"></i>
          <span class="font-weight-bold">Free shipping</span>
          {% if settings.enable_multilang %}
          <span class="lang1">{{ settings.header_top_text | split: '|' | first }}</span>
          <span class="lang2">{{ settings.header_top_text | split: '|' | last }}</span>
          {% else %}
          <span class="lang1">{{ settings.header_top_text | split: '|' | first }}</span>
          {% endif %}
          <span class="font-weight-semibold">{{settings.header_top_text_price}}</span>
          <span class="currency">{{cart.currency.symbol}}</span>        	
          {% if settings.header_top_link_text != blank %}
          {% if settings.enable_multilang %}
          <a href="{{ settings.header_top_link }}" rel="nofollow" class="link lang1">{{ settings.header_top_link_text | split: '|' | first }}</a>
          <a href="{{ settings.header_top_link }}" rel="nofollow" class="link lang2">{{ settings.header_top_link_text | split: '|' | last }}</a>
          {% else %}
          <a href="{{ settings.header_top_link }}" rel="nofollow" class="link">{{ settings.header_top_link_text | split: '|' | first }}</a>
          {% endif %}
          {% endif %}
        </p>
      </div>

    </div>
  </div>
  {% endif %}
  {% endif %}
  <div class="header-middle d-flex justify-content-between align-items-center">
    <div class="mobile-menu">
      <a href="#" rel="nofollow" class="open-m">
        <i class="fas fa-bars"></i>
      </a>
    </div>
    <div class="wrap-header-logo spacing">
      {% if settings.brand_type == 'image' %}
      <a href="/" rel="nofollow">
        {{ 'logo.png' | asset_url | img_tag: shop.name }}
        {% if settings.enable_retina %}
        {{ 'logo_2x.png' | asset_url | img_tag: shop.name }}
        {% endif %}
      </a>
      {% elsif settings.brand_text != "" %}
      <a class="logo-title" href="/" rel="nofollow">{{ settings.brand_text }}</a>
      {% else %}
      <a class="logo-title" href="/" rel="nofollow">{{shop.name}}</a>
      {% endif %}
    </div>
    <div class="form-search">
      <div class="wrap-input">
        {% include 'search-bar' %}
      </div>
    </div>
    <div class="wrap-form-mobile">
      <a href="#" rel="nofollow">
        <i class="fas fa-search"></i>
      </a>
    </div>
    <div class="wrap-track-order spacing"><a class="font-weight-bold" href="https://7shorten.com/witchcraft-tracking" rel="nofollow">Track your order</a></div>
    <div class="wrap-cart"><p class="top-cart">
      <a href="/cart" rel="nofollow" id="cartToggle">
        <span><i class="fas fa-shopping-cart"></i></span>
        <span id="cartCount">{{ cart.item_count }}</span>
      </a> 
      </p>
    </div>
  </div>

  <div class="header-bottom">
    <nav class="nav-bar" role="navigation">
      <div class="mobile-logo">
        <a href="/" rel="nofollow">{{ 'logo.png' | asset_url | img_tag: shop.name }}</a>
      </div>
      {% include 'site-nav' %}
      <div class="close-mobile-menu">
        <a href="#" rel="nofollow" class="close-m">
          <i class="fas fa-times"></i>
        </a>
      </div>
    </nav>

  </div>
  <div class="overlay-m"></div>

</header>
{% if settings.enable_multilang %}
<script>
  translator.doTranslate(".site-header");
</script>
{% endif %}
{%endcomment%}


<script>
  $(document).ready(function(){
    $(window).bind(('load'),function(){
        $('#select-box-style').on('change', function() {
          var value = $(this).find('option:selected').attr('data-value'),
              txtValue = $(this).find('option:selected').text(),
              modalValue = $('.modal-content[data-id="'+value+'"]');
          $('.modal-content').css('display','none');
          $('.hide-select #select-guide').remove();
          $('.size-modal-tlt,.size-modal-txt').empty();
          if(modalValue.length == 1){
            $('.size-modal-tlt').append(txtValue+'<span> size guide</span>');
            modalValue.css('display','block');
          }else{
            $('.size-modal-txt').empty().append('<p>We are updating <b>'+txtValue+'</b> size guide. Thank you for your patience.</p>');
          }
      	}).trigger('change');
    });  
});

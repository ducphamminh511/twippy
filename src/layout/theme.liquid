<!doctype html>
<!--[if lt IE 7]><html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]><html class="no-js lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]><html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if IE 9 ]><html class="ie9 no-js"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html lang="en"> <!--<![endif]-->
<head>
{% include 'booster-seo' %}

<!-- AMPIFY-ME:START -->
{% include 'ampify-me-snippet' %}
<!-- AMPIFY-ME:END -->


  <!-- Basic page needs ================================================== -->
  <meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=-100%, user-scalable=yes" />
	<meta name="format-detection" content="telephone=no">
  <!-- Title and description ================================================== -->
  {% comment %}<title>
  {{ page_title }}{% if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif %}{% if current_page != 1 %} &ndash; Page {{ current_page }}{% endif %}{% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}
  </title>{% endcomment %}
  {% include "trackify_x" %}

  


  {% if page_description %}
  {% comment %}<meta name="description" content="{{ page_description | escape }}">{% endcomment %}
  {% endif %}

  <!-- Product meta ================================================== -->
  {% include 'fb-open-graph-tags' %}
  {% include 'twitter-card' %}

  <!-- Helpers ================================================== -->
  <link rel="canonical" href="{{ canonical_url }}">
  
  <!-- Favicon -->
  <link rel="shortcut icon" href="{{ 'favicon.png' | asset_url }}" type="image/x-icon" />
  
   <!-- fonts -->
  {% include 'head_fonts' %}
  
   <!-- Styles -->
   {% include 'head_styles' %}
  
   <!-- Scripts -->
  {% include 'head_scripts' %}

  <!-- Header hook for plugins ================================================== -->
  {{ content_for_header }}
{%- render 'bold-common' -%}

  <!--[if lt IE 9]>
  {{ '//html5shiv.googlecode.com/svn/trunk/html5.js' | script_tag }}
  <![endif]-->

  {% comment %}
    If you store has customers disabled, you can remove the following JS file
  {% endcomment %}
  {% if template contains 'customers' %}
    {{ 'shopify_common.js' | shopify_asset_url | script_tag }}
    {{ 'customer_area.js'  | shopify_asset_url | script_tag }}
  {% endif %}


	{{ shop.metafields.loox["global_html_head"] }}

  {% render 'shogun-head' %}
{% section 'filter-menu-settings' %}
</head>

{% comment %}
  Add the page template as a class for easy page or template specific styling.
{% endcomment %}
<body id="{{ page_title | handle }}" class=" {% if customer %}customer-logged-in {% endif %}template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}" >
{% capture the_snippet_content %}{% render 'socialshopwave-helper' %}{% endcapture %}
            {% unless the_snippet_content contains 'Liquid error' %}
              {{ the_snippet_content }}
            {% endunless %}
  
  <nav class="cbp-spmenu cbp-spmenu-vertical cbp-spmenu-left" id="cbp-spmenu-s1">
    <div class="gf-menu-device-wrapper">
      <div class="gf-menu-device-container"></div>
    </div>             
  </nav>
  <div class="wrapper-container" id="wrapper">	
	{% include 'header' %}
      
    <div class="quick-view"></div>

    <main id="main" class="main-content {% if template contains 'index' or template contains 'contact' %}{% else %}{% endif %}" role="main">
      {{ content_for_layout }}
      {% include "arp" %}
    </main>

    {% include 'footer' %}

    {% if settings.enable_quick_view %}
      {% include 'quick-view-template' %}
    {% endif %}

    {% include 'footer_scripts' %}
    {% include 'newsletter' %}
  </div>


	{{ shop.metafields.loox["global_html_body"] }}
 {% include 'smile-initializer' %}
 {% include 'judgeme_core' %}

  {%- render 'bold-loyalties-widget' -%}
{% include 'lootly-launcher' %}
{% include 'gtranslate', float: 'yes' %}
</body>
</html>